numTrain = -1
numQueries = 1
mode = "features"                  # mode to get the features: histogram, wavelet, granulometry, features

[Histograms]
histogram_mode = "pyramid"          # simple, subimage, pyramid, pyramidFast
color_space = "YCrCb"                 # RGB, LAB, Luv, HSL, HSV, Yuv, XYZ, YCrCb
bin_num = 51
visualize = False
subdivision = 2                     # used for the histogram mode subimage
levels = 5                          # used for the histogram mode pyramid
preprocess = None                   # 'None', 'normalize', 'equalize'

[Granulometry]
bin_num = 100
visualize = False

[Wavelets]
method = "db"                     # 'haar', 'db', 'sym', 'coif', 'bior', 'rbio', 'dmey', 'gaus', 'mexh', 'morl', 'cgau', 'shan', 'fbsp', 'cmor'
levels = 1

[Features]
detect = "AKAZE"    # 'BoostDesc', 'BriefDescriptorExtractor',
                        # 'DAISY', 'FREAK', 'HarrisLaplaceFeatureDetector'
                        # 'LATCH', 'LUCID', 'PCTSignaturesSQFD', 'PCTSignatures'
                        # 'SIFT', 'SURF', 'StarDetector', 'VGG', 'AZEMAR'
compute= "BoostDesc"
matching = "BruteForce"           # BruteForce, BruteForce-L1, BruteForce-Hamming, BruteForce-Hamming(2), FlannBased
distance  = "add"                   # mean, add
k = -1

[Directories]
imdir_train = "./Dataset/museum_set_random/"
imdir_query = "./Dataset/query_devel_random/"

[Evaluate]
k = 5
eval_method = "bhattacharyya"                # euclidean, L1, x_sq, hist_intersect, kernhell, bhattacharyya, x_sq_alt, kl_div, correlate

[Visualization]
enabled = False 

[Save_Pickle]
save = True
pathOut = "./Dataset/pickle/"